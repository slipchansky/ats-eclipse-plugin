	<template id="table4" autoinsert="true" context="org.agileware.natural.cucumber.Cucumber.StepDescription" deleted="false" description="4 cols" enabled="true" name="Table Row">${cursor}${col_1_value} | ${col_2_value} | ${col_3_value} | ${col_4_value} |</template>
